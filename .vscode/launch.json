{
  "configurations": [
    {
      "type": "gdb",
      "request": "attach",
      "name": "Attach to gdbserver configuration",
      "executable": "${workspaceRoot}/userprog/build/kernel.o",
      "target": "localhost:1234",
      "remote": true,
      "cwd": "${workspaceRoot}",
      "valuesFormatting": "parseText"
    }
  ]
}
